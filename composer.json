{
  "name": "jumbodroid/sms",
  "description": "SMS Module - PHP",
  "license": "MIT",
  "keywords": [
    "SMS",
    "Jumbodroid",
    "PHP"
  ],
  "authors": [
    {
      "name": "Alois Odhiambo",
      "email": "rayalois22@gmail.com",
      "homepage": "https://github.com/rayalois22",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/config": "^7.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.3.1",
    "symfony/var-dumper": "^5.0.8"
  },
  "autoload": {
    "psr-4": {
      "Jumbodroid\\Sms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jumbodroid\\Sms\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
